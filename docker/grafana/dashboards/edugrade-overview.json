    {
      "title": "Total Requests",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 5 },
      "targets": [
        {
          "expr": "sum(edugrade_http_requests_total{route!=\"/metrics\"})",

    {
      "title": "Request Rate",
      "description": "Requests per second (excl. /metrics)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 5 },
      "targets": [
        {
          "expr": "sum(rate(edugrade_http_requests_total{route!=\"/metrics\"}[5m]))",
          "legendFormat": "req/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2,
          "color": { "mode": "fixed", "fixedColor": "purple" },
          "thresholds": {
            "steps": [{ "color": "purple", "value": null }]
          }
        }
      },
      "options": {
        "colorMode": "background_solid",
        "graphMode": "area",
        "textMode": "value_and_name",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "title": "Avg Response Time",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 5 },
      "targets": [
        {
          "expr": "sum(rate(edugrade_http_request_duration_seconds_sum{route!=\"/metrics\"}[5m])) / sum(rate(edugrade_http_request_duration_seconds_count{route!=\"/metrics\"}[5m]))",
          "legendFormat": "Avg"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 2 }
            ]
          },
          "color": { "mode": "thresholds" }
        }
      },
      "options": {
        "colorMode": "background_solid",
        "graphMode": "area",
        "textMode": "value_and_name",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "title": "Error Rate",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 5 },
      "targets": [
        {
          "expr": "sum(rate(edugrade_http_requests_total{status_code=~\"5..\"}[5m])) / sum(rate(edugrade_http_requests_total[5m])) * 100",
          "legendFormat": "Error %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          },
          "color": { "mode": "thresholds" },
          "noValue": "0%"
        }
      },
      "options": {
        "colorMode": "background_solid",
        "graphMode": "none",
        "textMode": "value_and_name",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "title": "Cache Hit Rate",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 5 },
      "targets": [
        {
          "expr": "rate(edugrade_cache_hits_total[5m]) / (rate(edugrade_cache_hits_total[5m]) + rate(edugrade_cache_misses_total[5m]))",
          "legendFormat": "Hit Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "decimals": 1,
          "noValue": "N/A",
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.8 }
            ]
          },
          "color": { "mode": "thresholds" }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdMarkers": true,
        "showThresholdLabels": false
      }
    },
